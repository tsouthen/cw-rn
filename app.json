{
  "expo": {
    "name": "Clever Weather",
    "slug": "cw-rn",
    "privacy": "public",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./assets/images/clever_weather.png",
    "scheme": "myapp",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ff8800"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.listotechnologies.cleverweather"
    },
    "android": {
      "package": "com.listotechnologies.cleverweather"
    },
    "primaryColor": "#ff8800",
    "androidStatusBar": {
      "backgroundColor": "#c55900"
    },
    "description": ""
  }
}